{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "ec2:*",
                "iam:PassRole",
                "iam:CreateRole",
                "iam:AttachRolePolicy",
                "logs:*",
                "cloudwatch:*",
                "vpc:*",
                "ssm:*"
            ],
            "Resource": "*"
        }
    ]
}